{
	"name": "openData_Export_Regions",
	"properties": {
		"linkedServiceName": {
			"referenceName": "providerPortalBlobStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"container": {
				"type": "string",
				"defaultValue": "open-data"
			},
			"folder": {
				"type": "string",
				"defaultValue": "export"
			},
			"filename": {
				"type": "string",
				"defaultValue": "regions.csv"
			}
		},
		"folder": {
			"name": "Open Data"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().folder",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().container",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "REGION_ID",
				"type": "String"
			},
			{
				"name": "SUBREGION_NAME",
				"type": "String"
			},
			{
				"name": "LATITUDE",
				"type": "String"
			},
			{
				"name": "LONGITUDE",
				"type": "String"
			},
			{
				"name": "POSTCODE",
				"type": "String"
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}