{
	"name": "openData_Export_Regions",
	"properties": {
		"linkedServiceName": {
			"referenceName": "providerPortalBlobStorage",
			"type": "LinkedServiceReference"
		},
		"folder": {
			"name": "openDataExpot"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": "regions.csv",
				"folderPath": {
					"value": "@concat('export/', formatDateTime(utcnow(), 'yyyy-MM-dd')) ",
					"type": "Expression"
				},
				"container": "open-data"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "Id",
				"type": "String"
			},
			{
				"name": "ApiLocationId",
				"type": "String"
			},
			{
				"name": "SubRegionName",
				"type": "String"
			},
			{
				"name": "Latitude",
				"type": "String"
			},
			{
				"name": "Longitude",
				"type": "String"
			}
		]
	}
}